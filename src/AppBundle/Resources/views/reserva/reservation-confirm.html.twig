{% extends 'base_admin.html.twig' %}
{% block body %}



    <div class="about-bottom" id="ab">
        <div class="col-md-12 ">
            <h3 class="tittle why">CONFIRMACION DE HABITACION</h3>
            <div class="book-form">
                <form id="reservas" class="form-horizontal">
                    <div class="form-group">
                        <label for="focusedinput" class="col-sm-2 control-label">Código</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control1" id="idreserva" placeholder="Ingrese código de reservación">
                        </div>
                        <br>
                    </div>
                    <div class="form-group">
                        <label for="focusedinput" class="col-sm-2 control-label">Pin </label>
                        <div class="col-sm-8">
                            <input type="password"  required pattern="[A-Za-z0-9]+" maxlength="4" class="form-control1" id="pinhotel" placeholder="Ingrese pin del hotel">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="make wow shake" data-wow-duration="1s" data-wow-delay=".5s">
                            <input type="submit" value="Buscar">
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="clearfix"></div>
        <div id="rooms" class="container">
            <!--Your code report confirm-reservation.html-->
        </div>
    </div>
    <script src="{{ asset('js/jquery-2.1.4.min.js') }}"></script>
    <script src="{{ asset('js/reservas.js') }}"></script>


{% endblock %}